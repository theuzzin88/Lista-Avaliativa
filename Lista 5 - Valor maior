#include <stdio.h>

int main() {
    int A, B;


    printf("Insira o valor de a: ");
    scanf("%d", &A);

    printf("Insira o valor de b: ");
    scanf("%d", &B);

    if (A == B) {

        printf("Os valores lidos sao iguais\n");
    } else {

        int maior, menor;
        if (A > B) {
            maior = A;
            menor = B;
        } else {
            maior = B;
            menor = A;
        }

        // Verificar se o maior valor é múltiplo do menor valor
        if (maior % menor == 0) {
            printf("%d eh maior e eh multiplo de %d\n", maior, menor);
        } else {
            printf("%d eh maior mas nao eh multiplo de %d\n", maior, menor);
        }
    }

    return 0;
}
